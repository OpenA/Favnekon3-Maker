{
	"manifest_version": 2,
	"name": "Favnekon3 Maker",
	"description": "__MSG_appDesc__",
	"version": "2.0",
	"default_locale": "en_US",
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [{
		"css": ["content_styles.css"],
		"js": ["content_script.js"],
		"matches": [ "*://*/*" ],
		"run_at": "document_start"
	}],
	"permissions": [
		"contextMenus",
		"<all_urls>"
	],
	"icons": {
		"16" : "push16.png",
		"48" : "push48.png",
		"128": "push128.png"
	}
}
